<!DOCTYPE html>
<html>
  <%- include('../head'); -%>
  <body>
    <%- include('../navbar', {active: 'users'}); -%>
    <div class="row">
      <div class="col p-5">
        <h1>Users</h1>
        <ul class="col p-3">
          <%_ for (const user of users) { -%>
            <form class="mb-3" id="<%= user.id -%>" >
              <div class="border rounded row" style="background-color: #bee5eb;">
                <div class="col">
                    <h1 class="py-3 pr-3"><%- user.first_name-%> <%- user.last_name-%></h1>
                </div>
                <h3 class="text-right pr-4 pt-4 col-6" >User Id: <%- user.user_id -%></h3>
              </div>
            </form>
          <%_ } -%>
      </div>
    </div>

  </body>
</html>
